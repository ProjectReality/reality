input.radio,
{
    width: 30px;
    height: 30px;

    vertical-align: -11px;
}

input.radio
{
    icon-decorator: image;
    icon-image: radio.tga 407px 0px 437px 30px;
}

input.radio:hover
{
    icon-image-s: 437px 467px;
}

input.radio:active
{
    icon-image-s: 467px 497px;
}

input.radio:checked
{
    icon-image-t: 30px 60px;
}

input.radio:checked:hover
{
    icon-image-s: 437px 467px;
    icon-image-t: 30px 60px;
}

input.radio:checked:active
{
    icon-image-s: 467px 497px;
    icon-image-t: 30px 60px;
}