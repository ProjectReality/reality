/*
* Default styles for all the basic elements.
*/

div
{
    display: block;
    font-charset: U+0020-00FF;
text-align: center;
}

p
{
    display: block;
}

h1
{
    display: block;
}

em
{
    font-style: italic;
}

strong
{
    font-weight: bold;
}

datagrid
{
    display: block;
}

select, dataselect, datacombo, inset
{
    color : white;
    width: 320px;
    height: 28;
    background : #39A61B;
    border-top: 2px #287513;
    border-bottom: 2px #287513;
    border-left: 2px #287513;
    border-right: 2px #287513;
    /* padding: 5px; */
    text-align: center;

}



/* Force the tabset element to a fixed size. */
tabset
{
    display: block;
    width: 80%;

    margin-left: auto;
    margin-right: auto;
    /* height: 200px; */
}

/* Display the tab container as a block element 20 pixels high; it will
   be positioned at the top of the tabset. */
tabset tabs
{
    display: block;
    height: 20px;
    width: 100%;
    color : white;
    background : #39A61B;
    border-top: 2px #287513;
    border-bottom: 2px #287513;
    border-left: 2px #287513;
    border-right: 2px #287513;
    padding: 5px;
    text-align: center;

    margin-left: auto;
    margin-right: auto;
}

/* Individual Tab */
tabset tab
{
    width: 84px;
    border-left: 2px #287513;
    border-right: 2px #287513;

    outline-style: double;
    outline-width: 8;

}

/* Display the panel container as a block element 180 pixels high; it will
   be positioned below the tab container and take up the rest of the space
   in the tabset. */
tabset panels
{
    display: block;
    height: 180px;
}

/* Fix each panel to take up exactly the panelled space. */
tabset panels panel
{
    display: block;
    width: 100%;
    height: 100%;
    color: white;

    font-size: 16;

    outline-font-effect: outline;
    outline-width: 1px;
    outline-color: #4A7D67;

    text-shadow: 2px 2px black;
}

button {
    color : white;
    background : #39A61B;
    border-top: 2px #287513;
    border-bottom: 2px #287513;
    border-left: 2px #287513;
    border-right: 2px #287513;
    text-align: center;
    padding: 2px;
}


